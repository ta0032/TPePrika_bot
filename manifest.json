{
  "manifest_version": 3,
  "name": "TPePrika",
  "description": "Они достанут тебя и здесь",
  "version": "1.0.0",
  "permissions": ["notifications"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": ["background.js"]
  }
}
